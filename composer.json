{
  "name": "siberfx/share",
  "description": "Share links with Laravel 10+",
  "keywords": [
    "share",
    "laravel10",
    "laravel11"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "siberfx",
      "email": "info@siberfx.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11|^10"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^0.9.4",
    "guzzlehttp/guzzle": ">=7.0"
  },
  "autoload": {
    "psr-0": {
      "Siberfx\\Share": "src/"
    },
    "classmap": [
        "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Siberfx\\Share\\ShareServiceProvider"
      ],
      "aliases": {
        "Share": "Siberfx\\Share\\Facade\\Share"
      }
    }
  },
  "minimum-stability": "stable"
}
